{
  "name": "@launchacademy/shortcut-cli",
  "version": "0.0.0",
  "description": "Query cli tool for shortcut",
  "engines": {
    "node": ">= 14.16 <15"
  },
  "bin": {
    "shortcut": "./build/src/main.js"
  },
  "devDependencies": {
    "@types/jest": "~26.0.23",
    "@types/node": "~14.14.45",
    "@types/yaml": "^1.9.7",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "~4.28.2",
    "@typescript-eslint/parser": "~4.28.2",
    "eslint": "~7.30.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.3.6",
    "jest": "~27.0.6",
    "prettier": "~2.3.2",
    "rimraf": "~3.0.2",
    "ts-jest": "~27.0.3",
    "ts-node": "^10.2.1",
    "tsutils": "~3.21.0",
    "typescript": "~4.3.5"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "yarn run rimraf coverage build tmp",
    "prebuild": "yarn run lint",
    "build": "yarn run tsc -p tsconfig.release.json",
    "build:watch": "yarn run tsc -w -p tsconfig.release.json",
    "lint": "yarn run eslint . --ext .ts,.tsx",
    "test": "yarn run jest --coverage",
    "test:watch": "yarn run jest --watch",
    "ci": "yarn run test"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.172",
    "chalk": "^4.1.2",
    "clubhouse-lib": "^0.12.0",
    "deepmerge": "^4.2.2",
    "tslib": "~2.3.0",
    "yaml": "^1.10.2",
    "yargs": "^17.1.1"
  }
}
